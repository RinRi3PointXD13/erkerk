local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Vu = game:GetService("VirtualUser")
game:GetService("Players").LocalPlayer.Idled:connect(function()
   Vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
   wait(1)
   Vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
end)

local colors = {
		SchemeColor = Color3.fromRGB(70, 70, 70),
		Background = Color3.fromRGB(31, 41, 43),
        Header = Color3.fromRGB(22, 29, 31),
        TextColor = Color3.fromRGB(255,255,255),
        ElementColor = Color3.fromRGB(22, 29, 31)
}

local Window = Library.CreateLib("ErkErk Hub", colors)

local Tab = Window:NewTab("Main")

local Section = Tab:NewSection("Farm")

local WalkSpeedSection = Tab:NewSection("WalkSpeed/JumpPower")

WalkSpeedSection:NewSlider("WalkSpeed", "ErkErk", 1000, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

WalkSpeedSection:NewSlider("JumpPower", "ErkErk", 1000, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)

function EspErkErk()
	for i,v in pairs(workspace:GetDescendants()) do
			if v.ClassName == "TouchTransmitter" and v.Parent.Name == "Handle" then
			if v:FindFirstChildOfClass("BillboardGui") then return end	
				local BillboardGui = Instance.new("BillboardGui")
				local TextLabel = Instance.new("TextLabel")
				wait()
				BillboardGui.Parent = v.Parent
				BillboardGui.AlwaysOnTop = true
				BillboardGui.LightInfluence = 1
				BillboardGui.Size = UDim2.new(0, 50, 0, 50)
				BillboardGui.StudsOffset = Vector3.new(0, 2, 0)

				TextLabel.Parent = BillboardGui
				TextLabel.BackgroundColor3 = Color3.new(1, 1, 1)
				TextLabel.BackgroundTransparency = 1
				TextLabel.Size = UDim2.new(1, 0, 1, 0)
				TextLabel.Text = v.Parent.Parent.Name
				TextLabel.TextColor3 = Color3.new(1, 0, 0)
				TextLabel.TextScaled = true
				TextLabel.Font = "Cartoon"
				TextLabel.TextStrokeTransparency = 0
				TextLabel.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
		end
	end
end

Section:NewButton("Esp", "ไม่ได้ถาม", function()
   EspErkErk()
end)

function tpitemErkErk()
	for i,v in pairs(workspace:GetChildren()) do
		if v:IsA("Tool") then
			local thing = v:FindFirstChild("Handle")
			if thing then
				game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(thing.CFrame)
			end
		end
	end
end

local autofarm = {}
autofarm.enabled = false

Section:NewToggle("Auto Farm", "โม้", function(state)
    if state then
    	autofarm.enabled = true
    	while autofarm.enabled do
    		wait()
    		tpitemErkErk()
		end   
    else
         autofarm.enabled = false
    end
end)

function automoneyErkErk()
	if game.Players.LocalPlayer.Backpack:FindFirstChild("money") then
		game.Players.LocalPlayer.Backpack["money"].Parent = game.Players.LocalPlayer.Character
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:IsA("Tool") and v.Name == "money" then
				v:Activate()
			end
		end
	end
end

function automoneyErkErk2()
	if game.Players.LocalPlayer.Backpack:FindFirstChild("Banknote") then
		game.Players.LocalPlayer.Backpack["Banknote"].Parent = game.Players.LocalPlayer.Character
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:IsA("Tool") and v.Name == "Banknote" then
				v:Activate()
			end
		end
	end
end

local automoney = {}
automoney.enabled = false
	
Section:NewToggle("Auto Collect Money", "โม้", function(state)
    if state then
    	automoney.enabled = true
    	while automoney.enabled do
    		wait()
    		if game.PlaceId == 5506995922 then
				automoneyErkErk()	
    		elseif game.PlaceId ~= 5506995922 then
    			automoneyErkErk2()	
			end
  
		end   
    else
         automoney.enabled = false
    end
end)

local Tab2 = Window:NewTab("Setting")

local Section2 = Tab2:NewSection("Setting")

Section2:NewKeybind("Keybind", "ErkErk", Enum.KeyCode.RightControl, function()
	Library:ToggleUI()
end)

local Tab3 = Window:NewTab("Remote Spy")
local Section3 = Tab3:NewSection("Spy")

Section3:NewButton("Open Gui", "ไม่ได้ถาม", function()
   loadstring(game:HttpGet("https://raw.githubusercontent.com/specowos/-not-mine-R2S/main/R2S-1.txt"))()
end)

local Tab4 = Window:NewTab("Troll")
local Section4 = Tab4:NewSection("Skill")

function Nuke(Dmg)
 for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren())do
	 if v:IsA("Part" or "MeshPart" or "Union") then
		 v.Anchored = false
	 end
end
	for i,hit in ipairs(game.Players:GetChildren())do
		if hit.Character:FindFirstChild("Humanoid") and hit.Character:FindFirstChild("HumanoidRootPart") and hit.Character.Name ~= game.Players.LocalPlayer.Name then
				game:GetService("ReplicatedStorage").Damage30:FireServer(hit.Character:FindFirstChild("Humanoid"), hit.Character.HumanoidRootPart.CFrame, Dmg, 0.1, Vector3.new(0,0,0), 5, "rbxassetid://5228745586", 1, 1)
		end
	end
end

local V1 = 1

function Heal(V)
	game:GetService("ReplicatedStorage").Heal:FireServer(game.Players.LocalPlayer.Character.HumanoidRootPart,V,"rbxassetid://5228745586",1,1)
end

local dmg = 0

Section4:NewSlider("NukeDamage", "ErkErk", 1000, 0, function(s)
  dmg = s
end)

Section4:NewSlider("HealValue", "ErkErk", 1000, 1, function(s1)
  V1 = s1
end)

local autonuke = {}
autonuke.enabled = false
	
Section4:NewToggle("Loop Nuke", "โม้", function(state)
    if state then
    	autonuke.enabled = true
    	while autonuke.enabled do
    		wait()
    		Nuke(dmg)
		end   
    else
         autonuke.enabled = false
    end
end)

local autoheal = {}
autoheal.enabled = false
	
Section4:NewToggle("Loop Heal", "โม้", function(state)
    if state then
    	autoheal.enabled = true
    	while autoheal.enabled do
    		wait()
    		Heal(V1)
		end   
    else
         autoheal.enabled = false
    end
end)

local autoblock = {}
autoblock.enabled = false
	
Section4:NewToggle("Always Block", "เอิ้กๆ", function(state)
    if state then
    	autoblock.enabled = true
    	while autoblock.enabled do
    		wait()
    		game:GetService("ReplicatedStorage").Block:FireServer(true)
		end   
    else
         autoblock.enabled = false
         game:GetService("ReplicatedStorage").Block:FireServer(false)
    end
end)

function AntiTS()
	for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
		game:GetService("ReplicatedStorage").Anchor:FireServer(v ,false)
	end
end

local Section5 = Tab4:NewSection("Misc")

Section5:NewButton("Nuke", "ไม่ได้ถาม", function()
  Nuke(dmg)
end)

Section5:NewButton("AntiTimeStop", "ไม่ได้ถาม", function()
		 AntiTS()
end)

Section5:NewButton("NoCoolDown", "ไม่ได้ถาม", function()
	loadstring((game:HttpGet('https://pastebin.com/raw/frJkW35h')))()
end)


